{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "popper.js@1.16.1",
      "destination": "wwwroot/lib/popper.js/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@3.1.4",
      "destination": "wwwroot/lib/signalr/",
      "files": [
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "library": "croppie@2.6.5",
      "destination": "wwwroot/lib/croppie/"
    },
    {
      "provider": "jsdelivr",
      "library": "barea.js@1.2.3",
      "destination": "wwwroot/lib/barea/",
      "files": [
        "barea.js",
        "barea.min.js",
        "LICENSE"
      ]
    }
  ]
}